#!/usr/bin/env bash

# This script is automatically generated by icub-firmware-build's CMake
# to permit to launch the FirmwareUpdater.scripts.* without the need to manually
# navigate to their folder

# Get location of current script
scriptDirectory="$( cd "$( dirname "${BASH_SOURCE[0]}" )" >/dev/null 2>&1 && pwd )"
icubFirmwareScriptsPath=`realpath $scriptDirectory/@ICUB_FIRMWARE_RELATIVE_PATH_BETWEEN_BIN_AND_ICUB_FIRMWARE_SCRIPTS@`

cd $icubFirmwareScriptsPath
bash ./@ICUB_FIRMARE_BASH_SCRIPT_NAME@
